  "main": "./dist/crazy.umd.js",
  "module": "./dist/crazy.es.js",
  "types": "./dist/crazy.d.ts",
  "exports": {
    ".": {
      "import": "./dist/crazy.es.js",
      "require": "./dist/crazy.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./assets/": "./dist/assets/"
  },
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "start:web": "yarn tsc && yarn vite && yarn workNameSpaceToFix && yarn typedoc",
    "dev": "vite serve --force --host --config ./config/vite.config.ts",
    "dev:patch": "vite serve --host --config ./config/vite.dev.config.ts",
    "vite": "vite build --mode NODE_ENV --config ./config/vite.config.ts && yarn run copyTS",
    "typedoc": "npx typedoc --tsconfig ./doc.tsconfig.json",
    "fix": "eslint --fix --ext .jsx src/**",
    "copyTS": "shx cp ./src/types/crazy.d.ts ./dist",
    "np": "npm publish --access-public",
    "patch": "yarn vite && yarn clean",
    "version:patch": "npm version patch",
    "clean": "node config/patchClean.js",
    "workNameSpaceToFix": "node ./config/workNameSpaceToFix.js",
    "tsc": "tsc -p ./tsconfig.json --isolatedModules false",
    "test": "jest --coverage"
  },
